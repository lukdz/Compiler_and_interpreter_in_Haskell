(*
oczekiwany wynik:
  - dla n < 0 -> n
  - dla 0 -> 0
  - dla 1 -> 1
  - dla 2 -> 1
  - dla 3 -> 2
  - dla 4 -> 3
  - dla 5 -> 5
  - dla 6 -> 8
  - dla 7 -> 13
  - dla 8 -> 21
  - dla 9 -> 34
  - dla 19 -> 4181
*)

fun fib(n : int) : int =
    if n <= 1 then n
    else fib(n-1) + fib(n-2)

in let n=9 in fib(n)
